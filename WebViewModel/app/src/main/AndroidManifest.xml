<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kurniawanharry165gmail.webviewmodel">

    <strong><uses-permission android:name="android.permission.INTERNET" /></strong>
    <strong><uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" /></strong>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Model1" />
        <activity
            android:name=".Model2"
            android:theme="@style/Fullscreen" />
        <activity android:name=".Model3"></activity>
    </application>

</manifest>